<?xml version="1.0" encoding="UTF-8"?>
<development-component
		version="1.0.5"
		xmlns="http://xml.sap.com/2002/11/DevelopmentComponent"
		xmlns:IDX="urn:sap.com:DevelopmentComponent:1.0"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://xml.sap.com/2002/11/DevelopmentComponent dcdef.xsd">
	<name>p_sfdc/bl/br/customer</name>
	<vendor>nobelbiocare.com</vendor>
	<component-type>
		<vendor>sap.com</vendor>
		<type>com.sap.brmside.rulesdctype</type>
	</component-type>
	<properties>
		<localization xmlns="http://xml.sap.com/2003/06/Localization" xmlns:ns0="http://xml.sap.com/2003/06/Localization">
			<domain>BC</domain>
			<originalLocale>en</originalLocale>
		</localization>
		<moin xmlns="http://xml.sap.com/2007/05/MoinModel">
			<moin_folders/>
			<moin_resource_folders>
				<folder path="mc"/>
				<folder path="rep"/>
				<folder path="src"/>
			</moin_resource_folders>
		</moin>
	</properties>
	<build-plugin>
		<dc-ref>
			<name>tc/brms/bi/bp/buildtool</name>
			<vendor>sap.com</vendor>
		</dc-ref>
		<pp-ref>def</pp-ref>
	</build-plugin>
	<dependencies>
		<dependency>
			<dc-ref>
				<name>bie/sca/scdl/contributors/rules</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>def</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>bie/sca/scdl/gen</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>def</pp-ref>
			<at-build-time/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>tc/bpem/content/predefs/types</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<at-deploy-time/>
			<at-runtime/>
		</dependency>
		<dependency>
			<dc-ref>
				<name>tc/bpem/content/predefs/types</name>
				<vendor>sap.com</vendor>
			</dc-ref>
			<pp-ref>def_cmp</pp-ref>
			<at-build-time/>
		</dependency>
	</dependencies>
	<folders>
		<source-folder>mc</source-folder>
		<source-folder>rep</source-folder>
		<source-folder>src</source-folder>
	</folders>
	<access-control-list>
		<grant forwarding-allowed="false">
			<sc-ref>
				<name>SC_SFDC</name>
				<vendor>nobelbiocare.com</vendor>
			</sc-ref>
		</grant>
	</access-control-list>
	<attributes>
		<attribute>
			<attribute-type>is_moin_dc</attribute-type>
			<attribute-vendor>sap.com</attribute-vendor>
			<attribute-value>true</attribute-value>
		</attribute>
	</attributes>
</development-component>
